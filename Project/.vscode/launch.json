    "version": "0.2.0",
    "configurations": [



        {
            "name": "Launch program(from main.py)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\my_code\\main.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}\\my_code"
        },
        {
            "name": "Loop program(from loop.py)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\my_code\\loop.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}\\my_code"
        },
        {
            "name": "Launch training test(from simple_train.py)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\simple_train\\simple_train.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Debug: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}\\my_code"
        }
    ]
}
